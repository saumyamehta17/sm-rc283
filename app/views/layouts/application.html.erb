<!DOCTYPE html>
<html>
<head>
  <title>SmRc283</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if current_user %>
    <%= link_to 'log_out' %>
<% else %>
    <%= link_to 'sign_up' , sign_up_path %> or
    <%= link_to 'log_in' , login_path %>
<% end %>
<%= yield %>

</body>
</html>
